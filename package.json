{
  "name": "cucumber-html-reports-server",
  "version": "1.0.9",
  "description": "Generate beautifull cucumberjs reports for multiple instances (browsers / devices)",
  "keywords": [
    "cucumber",
    "html",
    "test report",
    "multiple-cucumber-html-reporter",
    "html report",
    "json to html",
    "server"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "startServer": "./bin/start-server.js"
  },
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true mocha",
    "test-cover": "cross-env TS_NODE_FILES=true nyc --clean npm run test",
    "eslint-fix-all": "npx eslint src/lib/**/**/**/*.ts src/lib/**/**/*.ts test/**/*.ts --fix",
    "pretest": "npm run eslint-fix-all && npm run build",
    "create-package": "npx tsc -p tsconfig.json && npm publish",
    "generate-report": "node ./test/test.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -V ./src/lib/server/views/*.ejs ./src/lib/generate-report/src/resources/templates/components/**/*.tmpl ./src/lib/generate-report/src/resources/templates/components/**/**/*.tmpl ./src/lib/generate-report/src/resources/templates/scripts/*.tmpl ./src/lib/generate-report/src/resources/templates/style.css dist",
    "build": "npm run-script clean && tsc && npm run-script copy-files",
    "update-package-json-dependencies": "npx ncu -u && npm i",
    "update-report-resources": "ts-node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/resources/scripts/ts/update-report-resources.ts",
    "update-app": "npm run-script update-package-json-dependencies && npm run-script update-report-resources",
    "start:server": "run-script-os",
    "start:server:windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./src/scripts/startServer.ps1",
    "start:server:linux": "./src/scripts/startServer.sh",
    "release": "release-it --github.release",
    "release.ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release.patch": "npm run release -- patch --ci",
    "release.minor": "npm run release -- minor --ci",
    "release.major": "npm run release -- major --ci",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yon-cuadrado/html-cucumber-report-generator"
  },
  "author": "Yon Cuadrado",
  "dependencies": {
    "axios": "0.27.2",
    "cucumber": "6.0.5",
    "cucumber-html-report-generator": "1.0.16",
    "date-fns": "^2.28.0",
    "ejs": "3.1.8",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "gherkin": "9.0.0",
    "http-server": "14.1.0",
    "jade": "1.11.0",
    "json-future": "2.2.18",
    "lodash": "4.17.21",
    "mongoose": "6.3.4",
    "open": "8.4.0",
    "pug": "3.0.2",
    "run-script-os": "1.1.6",
    "save-dev": "^0.0.1-security",
    "shelljs": "0.8.5",
    "tsconfig-paths": "4.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.1",
    "@types/chai-as-promised": "7.1.5",
    "@types/chai-fs": "2.0.2",
    "@types/cucumber": "6.0.1",
    "@types/date-and-time": "0.13.0",
    "@types/express": "4.17.13",
    "@types/filesystem": "0.0.32",
    "@types/fs-extra": "9.0.13",
    "@types/http-server": "0.12.1",
    "@types/jquery": "3.5.14",
    "@types/js-base64": "3.0.0",
    "@types/lodash": "4.14.182",
    "@types/mocha": "9.1.1",
    "@types/mongoose": "5.11.96",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.8",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "chai-http": "4.3.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "fs-plus": "3.1.1",
    "husky": "^8.0.1",
    "jshint": "2.13.4",
    "mocha": "10.0.0",
    "mocha-explorer-launcher-scripts": "0.4.0",
    "mocha-sinon": "2.1.2",
    "npm-check-updates": "13.0.3",
    "nyc": " 15.1.0",
    "prettier": "2.6.2",
    "release-it": "^15.0.0",
    "rimraf": "3.0.2",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.8.0",
    "tslint": "6.1.3",
    "tslint-to-eslint-config": "2.13.0",
    "typescript": "4.6.4",
    "yargs": "17.5.1"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "license": "ISC"
}
