{
  "name": "cucumber-html-reports-server",
  "version": "0.0.1",
  "description": "Generate beautifull cucumberjs reports for multiple instances (browsers / devices)",
  "keywords": [
    "cucumber",
    "html",
    "test report",
    "multiple-cucumber-html-reporter",
    "html report",
    "json to html",
    "server"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true mocha",
    "test-cover": "cross-env TS_NODE_FILES=true nyc --clean npm run test",
    "eslint-fix-all": "npx eslint src/lib/**/**/**/*.ts src/lib/**/**/*.ts test/**/*.ts --fix",
    "pretest": "npm run eslint-fix-all && npm run build",
    "create-package": "npx tsc -p tsconfig.json && npm publish",
    "generate-report": "node ./test/test.js",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -V ./src/lib/server/views/*.ejs ./src/lib/generate-report/src/resources/templates/components/**/*.tmpl ./src/lib/generate-report/src/resources/templates/components/**/**/*.tmpl ./src/lib/generate-report/src/resources/templates/scripts/*.tmpl ./src/lib/generate-report/src/resources/templates/style.css dist",
    "build": "npm run-script clean && tsc && npm run-script copy-files",
    "update-package-json-dependencies": "npx ncu -u && npm i",
    "update-report-resources": "ts-node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/scripts/update-report-resources.ts",
    "update-app": "npm run-script update-package-json-dependencies && npm run-script update-report-resources"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yon-cuadrado/html-cucumber-report-generator"
  },
  "author": "Yon Cuadrado",
  "dependencies": {
    "axios": "0.24.0",
    "cucumber": "6.0.5",
    "cucumber-html-report-generator": "0.0.17",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "gherkin": "9.0.0",
    "http-server": "14.0.0",
    "jade": "1.11.0",
    "json-future": "2.2.14",
    "lodash": "4.17.21",
    "mongoose": "^6.0.12",
    "open": "8.4.0",
    "pug": "3.0.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.22",
    "@types/chai-as-promised": "7.1.4",
    "@types/chai-fs": "2.0.2",
    "@types/cucumber": "6.0.1",
    "@types/date-and-time": "0.13.0",
    "@types/express": "4.17.13",
    "@types/filesystem": "0.0.32",
    "@types/fs-extra": "9.0.13",
    "@types/http-server": "0.12.1",
    "@types/jquery": "3.5.8",
    "@types/js-base64": "3.0.0",
    "@types/lodash": "4.14.176",
    "@types/mocha": "9.0.0",
    "@types/mongoose": "5.11.96",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "3.2.5",
    "@types/ssri": "^7.1.1",
    "@types/yargs": "17.0.5",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "chai-http": "^4.3.0",
    "chart.js": "^3.6.0",
    "copyfiles": "2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-standard": "4.1.0",
    "fs-plus": "3.1.1",
    "jshint": "2.13.1",
    "mocha": "9.1.3",
    "mocha-sinon": "2.1.2",
    "moment": "2.29.1",
    "npm-check-updates": "11.8.5",
    "nyc": " 15.1.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.20",
    "ssri": "^8.0.1",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "tslint": "6.1.3",
    "tslint-to-eslint-config": "2.10.1",
    "typescript": "4.4.4",
    "yargs": "17.2.1"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "license": "ISC"
}
